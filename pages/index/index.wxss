.container {
  background-color: #fff;
  min-height: 100%;
  width: 750rpx;
  overflow: hidden;
  display: block;
}
.category-box {  
  background-color: #fff;
  clear: both;
}
.category-list{
  width:370rpx;
  margin-top:20rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}
.category-column{
  width:100%;
  margin-top:20rpx;
  overflow: hidden;
}
.category-imgbox{
  width:100rpx;
  height:100rpx;
}
.category-title{
  font-size: 24rpx;
  text-align:center;
  margin-top:10rpx;
}


.swiper-container{
  width: 750rpx;
  position: relative;
  padding-top: 35rpx;
  background: #ffffff url('https://dcdn.it120.cc/2019/08/20/dba77291-b1dd-4921-9d43-3142b0a663fb.png') no-repeat center;
  background-size: contain;
  background-position: 0 -2rpx;
}
.swiper_box {
  width: 750rpx;
  height: 430rpx;
  border-radius: 24rpx;
  overflow: visible;
}

swiper-item {
  display: flex;
  align-items: flex-start;
  justify-content: center;
}
swiper-item image {
  width: 666rpx;
  height:300rpx;
  border-radius: 24rpx;
  box-shadow: 0 15px 40px 0 rgba(0,0,0,0.08);
}
.swiper-container .dots{  
  position: absolute;  
  left: 0;  
  right: 0;  
  top: 300rpx;
  bottom: 20rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 8rpx;  
  width: 14rpx;  
  height: 14rpx;  
  background: rgb(133, 133, 133);  
  border-radius: 50%;  
  transition: all .6s;  
  opacity: .5;
}  
.swiper-container .dots .dot.active{  
  width: 14rpx;  
  background: rgb(56, 56, 56); 
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.type-item-on{
  color:#e64340;
  border-bottom: 1rpx solid #e64340;
}
.weui-loadmore {
  margin-bottom: 0px;
}
.goods-container{
  display: flex;
  justify-content: space-between;  
  flex-wrap:wrap;
  box-sizing: content-box;
  padding: 42rpx;
  /* background-color: #F2f2f2; */
}
.goods-box{
  width: 313rpx;
  height: 420;
  overflow: hidden;
  margin-bottom: 30rpx;
  padding-bottom: 12rpx;
  border-radius:12rpx;
